/*******************		Application wide			************************/

.dataTables_wrapper .dataTables_paginate .paginate_button
{
    border-radius: 5px !important;
    color: #fff !important;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    background-color: rgb(242,41,160);
    *background-color: rgb(242,41,160);
    background-image: -webkit-gradient(linear,0 0,0 100%,from(#F229A0),to(#F229A0));
    background-image: -webkit-linear-gradient(top,#F229A0,#F229A0);
    background-image: -o-linear-gradient(top,#F229A0,#F229A0);
    background-image: linear-gradient(to bottom,#F229A0,#F229A0);
    background-image: -moz-linear-gradient(top,#F229A0,#F229A0);
    background-repeat: repeat-x;
    border-color: #F229A0 #F229A0 #F229A0;
    border-color: rgba(0,0,0,0.1) rgba(0,0,0,0.1) rgba(0,0,0,0.25);
}

.dataTables_wrapper .dataTables_paginate .paginate_button:hover
{
    border-radius: 5px !important;
    color: #fff !important;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    background-color: rgb(242,41,170);
    *background-color: rgb(242,41,170);
    background-image: -webkit-gradient(linear,0 0,0 100%,from(#F229A1),to(#F229A0));
    background-image: -webkit-linear-gradient(top,#F229A1,#F229A0);
    background-image: -o-linear-gradient(top,#F229A1,#F229A0);
    background-image: linear-gradient(to bottom,#F229A1,#F229A0);
    background-image: -moz-linear-gradient(top,#F229A1,#F229A0);
    background-repeat: repeat-x;
    border-color: #F229A0 #F229A0 #F229A0;
    border-color: rgba(0,0,0,0.1) rgba(0,0,0,0.1) rgba(0,0,0,0.25);
}

.dataTables_wrapper .dataTables_paginate .paginate_button.disabled, .dataTables_wrapper .dataTables_paginate .paginate_button.disabled:hover, .dataTables_wrapper .dataTables_paginate .paginate_button.disabled:active
{
    border-radius: 5px !important;
    cursor: default;
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    background-color: rgb(242,41,170);
    *background-color: rgb(242,41,170);
    background-image: -webkit-gradient(linear,0 0,0 100%,from(#F229A1),to(#F229A0));
    background-image: -webkit-linear-gradient(top,#F229A1,#F229A0);
    background-image: -o-linear-gradient(top,#F229A1,#F229A0);
    background-image: linear-gradient(to bottom,#F229A1,#F229A0);
    background-image: -moz-linear-gradient(top,#F229A1,#F229A0);
    background-repeat: repeat-x;
    border-color: #F229A0 #F229A0 #F229A0;
    border-color: rgba(0,0,0,0.1) rgba(0,0,0,0.1) rgba(0,0,0,0.25);
}

