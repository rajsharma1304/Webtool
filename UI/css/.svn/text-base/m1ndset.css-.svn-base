.login_page form {
background: #f7f7f7 !important;
}

.login_logo 
{
	/* background-image: url(../img/bg_m.png) !important; */
	height:100px;
	padding:0 20px;
	-moz-border-radius-topleft: 6px;
	-moz-border-radius-topright: 6px;
	-moz-border-radius-bottomright: 0px;
	-moz-border-radius-bottomleft: 0px;
	-webkit-border-radius: 6px 6px 0px 0px;
	border-radius: 6px 6px 0px 0px;	
	position:relative;
	width:338px;
	margin:0 auto 24px;
	top: 11% !important;
}

.login_page .login_box
{
	top: 35% !important;
}	

.login_content 
{
	background-image: url(../img/bg_m.png) !important;
	/* background-color:#2E8EB7  !important; */
	background-repeat: repeat !important;
	background-attachment: fixed !important;
}

.btn-blue {
color: #fff;
text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
background-color: #49afcd;
background-image: -webkit-gradient(linear,0 0,0 100%,from(#2E8EB7),to(#2f96b4));
background-image: -webkit-linear-gradient(top,#2E8EB7,#2f96b4);
background-image: -o-linear-gradient(top,#2E8EB7,#2f96b4);
background-image: linear-gradient(to bottom,#2E8EB7,#2f96b4);
background-image: -moz-linear-gradient(top,#2E8EB7,#2f96b4);
background-repeat: repeat-x;
border-color: #2f96b4 #2f96b4 #1f6377;
border-color: rgba(0,0,0,0.1) rgba(0,0,0,0.1) rgba(0,0,0,0.25);
filter: progid:dximagetransform.microsoft.gradient(startColorstr='#ff5bc0de',endColorstr='#ff2f96b4',GradientType=0);
filter: progid:dximagetransform.microsoft.gradient(enabled=false);
}
.btn-blue:hover, .btn-blue:active, .btn-blue.active, .btn-blue.disabled, .btn-blue[disabled] {background-color: #2f96b4}
.btn-blue:hover {color:#fff;text-shadow: 0px -1px 0px #003151}
.btn-blue:focus {color: #fff;-webkit-box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05);-moz-box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05);box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05); border-color: #006D8D #004f6f #004f6f #006D8D; border-color: rgba(0, 0, 0, 0.25) rgba(0, 0, 0, 0.35) rgba(0, 0, 0, 0.35) rgba(0, 0, 0, 0.25); }
.btn-blue.active, .btn-blue:active { background-image: none;-webkit-box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05);-moz-box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05);box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05); color: #64d1f1; }



