/*******************		DASHBOARD			************************/
.no-show {
display:none !important;
}

.main_content 
{
	background-image: url(../img/bg_e.png) !important;
	background-repeat: repeat !important;
	background-attachment: fixed !important;
}
.sidebar_hidden #maincontainer
{
	background-color: none;
	background-image: url(../img/bg_e.png) !important;
}

.dshb_icoNav li a
{
	width: 105px !important;	
}

.dshb_icoNav li 
{	
	background: -moz-linear-gradient(top, #8cc7e0 0%, #b4dbeb 100%) !important;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#8cc7e0), color-stop(100%,#b4dbeb)) !important;
	background: -webkit-linear-gradient(top, #8cc7e0 0%,#b4dbeb,100%) !important;
	background: -o-linear-gradient(top, #8cc7e0 0%,#b4dbeb 100%) !important;
	background: -ms-linear-gradient(top, #8cc7e0 0%,#b4dbeb 100%) !important;
	background: linear-gradient(top, #8cc7e0 0%,#b4dbeb 100%) !important;
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#8cc7e0', endColorstr='#b4dbeb',GradientType=0 ) !important;
	border:1px solid #e2e2e2;
	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;-ms-border-radius: 6px;
	border-radius: 6px;
}

.dshb_icoNav li:hover {border-color:#2E8EB7  !important;-moz-box-shadow: 0 0 6px #ccc;-webkit-box-shadow: 0 0 6px #ccc;box-shadow: 0 0 6px #ccc}
.dshb_icoNav li a:hover {text-decoration:none}

